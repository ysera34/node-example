{
  "name": "go2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ./server/index.js --name go2 --interpreter babel-node --watch",
    "build": "babel ./serve -d ./dist",
    "serve": "node ./dist/index.js",
    "monit": "pm2 monit",
    "logs": "pm2 logs go2",
    "clear": "pm2 flush",
    "stop": "pm2 delete go2",
    "debug": "pm2 start ./server/index.js --name go2 --interpreter babel-node && pm2 logs go2",
    "test": "mocha --ui tdd --require babel-core/register --recursive ./server/**"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "mysql2": "^1.5.3",
    "pm2": "^3.0.3",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "nodemon": "^1.17.5"
  }
}
